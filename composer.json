{
    "name": "hexlet/code",
    "description": "Вычислитель отличий",
    "license": "proprietary",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Differ\\": "src/"
        },
        "files": [
            "src/Differ.php",
            "src/Parsers.php",
            "src/Formatters.php",
            "src/Formatters/Stylish.php",
            "src/Formatters/Plain.php",
            "src/Formatters/JSON.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Differ\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "aralts",
            "email": "aralts@yandex.ru"
        }
    ],
    "bin": [
        "bin/gendiff"
    ],
    "require": {
        "docopt/docopt": "^1.0",
        "squizlabs/php_codesniffer": "^3.10.2",
        "symfony/yaml": "^6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    }
}
